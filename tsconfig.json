{
  "compilerOptions": {
    "types": [
      "meteor",
    ],
    "lib": [
      "es2017",
      "es2017.string",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "jsx": "react",
    "paths": {
      "react": [
        "node_modules/react"
      ],
      "react-dom": [
        "node_modules/react-dom"
      ],
      "meteor/*": [
        "meteor/*",
      ],
    }
  },
}